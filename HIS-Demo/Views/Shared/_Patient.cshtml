<script type="text/javascript">

    function Send() {
        alert("You Add Successfully PatientSetup");
    }

    $(document).ready(function () {


        $("#vtabs4").jVertTabs({ equalHeights: true });

        index = 0;
        var count = 1;


        $("input[id^=next]").each(function (index) {
            $(this).click(function () {
                $("#0").click(function () { index = 0; index++; })
                $("#1").click(function () { index = 1; index++; })
                if (index > count) {
                    index = 0;

                } else {
                    $("#vtabs4").jVertTabs('selected', index);


                    index++;

                }


            });
        });

    });
    $(document).ready(function () {
        $("#HomeLink").click(function () {
            window.location.href = $(this).attr("title");
            return false;

        });
    });
</script>
